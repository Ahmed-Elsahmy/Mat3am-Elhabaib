@model IEnumerable<User>
@{
    ViewData["Title"] = "Users";
}



<div class="row">
    <div class="col-md-8 offset-2">
        <p>
            <h4 style="color:white">List of all your orders</h4>
        </p>

        <table class="table">
            <thead>
                <tr>
                    <th>User Name</th>
                    <th>User Email</th>
                    <th>User Phone Number</th>

                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.UserName</td>
                        <td class="align-middle">
                           @item.Email
                        </td>
                        <td class="align-middle">
                            @item.PhoneNumber
                        </td>
                    </tr>
                }
            </tbody>
        </table>

    </div>
</div>
<style>
    .table{
        color:white;
    }
</style>

